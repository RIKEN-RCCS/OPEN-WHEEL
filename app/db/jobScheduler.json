{
  "ParallelNavi": {
    "submit": "pjsub -X -Lelapse=00:30:00",
    "queueOpt": "-Lrscgrp=",
    "stat": "pjstat -H day=3 --choose st,ec",
    "del": "pjdel",
    "reJobID": "pjsub Job (\\d+) submitted.",
    "reFinishedState": "^ST *EC\\nEXT *0",
    "reReturnCode": "^ST *EC\\nEXT (\\d+)",
    "reFailedState": "^ST *EC\\n(CCL|ERR|EXT|RJT)"
  },
  "PBSPro":{
    "submit": "qsub",
    "queueOpt": "-q",
    "stat": "qstat -xf",
    "del": "qdel",
    "reJobID": "(\\d+).*",
    "reFinishedState": "substate \\= 92$",
    "reReturnCode": "Exit_status \\= (\\d+)$",
    "reFailedState": "substate \\= (91|93)$"
  },
  "SLURM":{
    "submit": "sbatch",
    "queueOpt": "-p",
    "stat": "sacct -P --delimiter=, -o State,ExitCode -n -j ",
    "del": "scancel",
    "reJobID": "Submitted batch job (\\d+)",
    "reFinishedState": "^COMPLETED,0",
    "reReturnCode": " .*(\\d+):\\d+$",
    "reFailedState": "^(COMPLETED|FAILED|CANCELLED|DEADLINE|NODE_FAIL|TIMEOUT)"
  }
}
