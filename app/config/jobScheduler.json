{
  "PBSPro" : {
    "submit": "qsub",
    "queueOpt": "-q",
    "stat": "qstat -xf",
    "del": "qdel",
    "reJobID": "^(\\d+).*",
    "reFinishedState": "substate \\= (91|92|93)$",
    "reReturnCode": "Exit_status \\= (\\d+)$",
    "reJobStatusCode": "substate \\= (\\d+)$",
    "grpName":""
  },
  "PBSProWithoutHistory" : {
    "submit": "qsub",
    "queueOpt": "-q",
    "stat": "qstat -xf",
    "del": "qdel",
    "reJobID": "^(\\d+).*",
    "reFinishedState": "Unknown Job Id",
    "reReturnCode": "",
    "reJobStatusCode": "",
    "acceptableRt": 153,
    "grpName":""
  },
  "SLURM":{
    "submit": "sbatch",
    "queueOpt": "-p",
    "stat": "sacct -P --delimiter=, -o JobID,State,ExitCode -n -j ",
    "del": "scancel",
    "reJobID": "Submitted batch job (\\d+)",
    "reFinishedState": "^\\d+,(?:COMPLETED|FAILED|CANCELLED|DEADLINE|NODE_FAIL|TIMEOUT),\\d+:\\d+",
    "reReturnCode": "^\\d+,\\S+,(\\d+):\\d+$",
    "reJobStatusCode": "^\\d+,(\\S+),\\d+:\\d+$",
    "grpName":""
  },
  "Fugaku": {
    "submit": "pjsub -X",
    "queueOpt": "-L rscgrp=",
    "stat": ["pjstat -v --choose jid,st,ec,pc","pjstat -v -H day=3 --choose jid,st,ec,pc"],
    "del": "pjdel",
    "reJobID": "pjsub Job (\\d+) submitted.",
    "reFinishedState": "^JOB_ID *ST *EC *PC *\\n\\d+ *(?:CCL|ERR|EXT|RJT) *\\d+ *\\d+",
    "reReturnCode":    "^JOB_ID *ST *EC *PC *\\n\\d+ *(?:EXT|CCL) *(\\d+) *\\d+",
    "reJobStatusCode": "^JOB_ID *ST *EC *PC *\\n\\d+ *\\S+ *\\d+ *(\\d+)",
    "grpName":""
  },
  "TCS": {
    "submit": "pjsub -X",
    "queueOpt": "-L rscgrp=",
    "stat": "pjstat -v -H day=3 --choose st,ec,pc",
    "del": "pjdel",
    "reJobID": "pjsub Job (\\d+_*\\d*) submitted.",
    "reFinishedState": "^ST *EC *PC *\\nEXT *0",
    "reFinishedStateStep": "^ST *EC *PC *\\nCCL *(\\d+) *6",
    "reReturnCode": "^ST *EC *PC *\\n(EXT|CCL) *(\\d+) *(\\d+)",
    "reFailedState": "^ST *EC *PC *\\n(CCL|ERR|EXT|RJT)",
    "reJobStatusCode": "^ST *EC *PC *\\n(\\S+)",
    "grpName":""
  },
  "UGE": {
    "submit": "qsub",
    "queueOpt": "-l ",
    "stat": ["qstat -j","qacct -j"],
    "del": "qdel",
    "reJobID": "(\\d+).*",
    "reFinishedState": "failed *0",
    "reFailedState": "failed *(\\d+)",
    "reReturnCode": "exit_status *(\\d+)",
    "reJobStatusCode": "exit_status *(\\d+)",
    "grpName":"-g"
  }
}
