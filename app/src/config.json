{
  "extension": {
    "project": ".prj.json",
    "workflow": ".wf.json"
  },
  "default_filename": "define",
  "submit_script": "submit.sh",
  "system_name": "swf",
  "node_color": {
    "task": "#FF0000",
    "workflow": "#0066FF",
    "for": "#0000FF",
    "while": "#00FFCC",
    "condition": "violet",
    "if": "#00FF66",
    "foreach": "#00FF00",
    "parameterstudy": "#6600FF"
  },
  "box_appearance": {
    "opacity":  0.6,
    "strokeWidth":  2,
    "titleHeight":  20,
    "marginHeight":  12,
    "outputTextOffset":  12,
    "textHeightScale": 1.2,
    "stepSize":  25
  },
  "state_color": {
    "planning": "white",
    "running": "cyan",
    "rerunning": "orange",
    "waiting": "yellow",
    "completed": "#00ff00",
    "failed": "red"
  },
  "plug_color": {
    "file": "cyan",
    "files": "magenta",
    "directory": "yellow",
    "flow": "orange",
    "elseFlow": "yellow"
  },
  "reload_project_ms": 2000
}
