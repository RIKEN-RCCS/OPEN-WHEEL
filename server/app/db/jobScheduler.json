{
  "PBSPro": {
    "submit": "qsub",
    "queueOpt": "-q",
    "stat": "qstat -xf",
    "del": "qdel",
    "grpName": "",
    "reJobID": "^(\\d+).*",
    "reRunning": "substate \\= (91|92|93)$",
    "reReturnCode": "Exit_status \\= (\\d+)$",
    "reJobStatusCode": "substate \\= (\\d+)$"
  },
  "SLURM": {
    "submit": "sbatch",
    "queueOpt": "-p",
    "stat": "sacct -P --delimiter=, -o JobID,State,ExitCode -n -j ",
    "del": "scancel",
    "grpName": "",
    "reJobID": "Submitted batch job (\\d+)",
    "reRunning": "^\\d+,(?:CONFIGURING|COMPLETING|PENDING|REVOKED|RUNNING|SPECIAL_EXIT|SUSPENDED),\\d+:\\d+",
    "reReturnCode": "^\\d+,\\S+,(\\d+):\\d+$",
    "reJobStatusCode": "^\\d+,(\\S+),\\d+:\\d+$"
  },
  "Fugaku": {
    "submit": "pjsub -X",
    "queueOpt": "-L rscgrp=",
    "stat": "pjstat -v --choose jid,st,ec,pc",
    "statAfter": "pjstat -v -H day=3 --choose jid,st,ec,pc",
    "del": "pjdel",
    "reJobID": "pjsub Job (\\d+_*\\d*) submitted.",
    "grpName": "",
    "bulkjob": true,
    "stepjob": true,
    "bulkstat": "pjstat -E -v --choose jid,st,ec,pc",
    "bulkstatAfter": "pjstat -E -v -H day=3 --choose jid,st,ec,pc",
    "reRunning": "^JOB_ID *ST *EC *PC *\\n(?:\\d+|\\d+_\\d+) *(?:CCL|ERR|EXT|RJT) *(?:\\d+|\\S+) *(?:\\d+|\\S+)",
    "reReturnCode": "^JOB_ID *ST *EC *PC *\\n(?:\\d+|\\d+_\\d+|\\d+\\[\\d+\\]) *(?:EXT|CCL) *(\\d+) *(?:\\d+)",
    "reJobStatusCode": "^JOB_ID *ST *EC *PC *\\n(?:\\d+|\\d+_\\d+|\\d+\\[\\d+\\]) *(?:CCL|ERR|EXT|RJT) *\\d+ *(\\d+)",
    "reSubReturnCode": "(?:\\d+\\[\\d+\\]) *(?:CCL|ERR|EXT|RJT) *(\\d+) *\\d+",
    "reSubJobStatusCode": "(?:\\d+\\[\\d+\\]) *(?:CCL|ERR|EXT|RJT) *\\d+ *(\\d+)"
  }
}
