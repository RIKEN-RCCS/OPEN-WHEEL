{
  "name": "WHEEL",
  "version": "1.2.3",
  "description": "Scientific Workflow system",
  "main": "app/index.js",
  "bin": {
    "wheel": "app/cmd.js"
  },
  "author": {
    "name": "Kenji Ono"
  },
  "contributors": [
    "Tomohiro Kawanabe",
    "tsuruta.kazuya",
    "Naoyuki Sogo"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "node app/index",
    "memoryInspection": "run-s memoryMonitor",
    "memoryMonitor": "cross-env NODE_ENV=development MEMORY_MONITOR=TRUE node app/index",
    "profiler": "node --prof app/index",
    "mocha": "mocha",
    "lint": "eslint --fix app/*.js app/routes/*.js app/db/*.js test/UT/*/*.js",
    "esw": "esw --no-color --watch --clear --fix app/*.js app/routes/*.js app/db/*.js",
    "coverage": "nyc --reporter=html --reporter=text npm run fullTest",
    "coverage:server": "nyc --reporter=html --reporter=text npm run UT:server",
    "coverage:client": "nyc --reporter=html --reporter=text npm run UT:client",
    "coverage:E2E": "nyc --reporter=html --reporter=text npm run E2E",
    "fullTest": "run-p UT:server UT:client E2E",
    "UT:server": "cross-env WHEEL_DISABLE_LOG=1  WHEEL_DISABLE_CONSOLE_LOG=1 mocha --recursive test/UT/server/",
    "UT:client": "opener test/UT/client/index.html",
    "E2E": "wdio test/E2E/wdio.conf.js",
    "prepare": "webpack",
    "watch": "webpack -w",
    "dev": "run-p watch node-dev",
    "node-dev": "node-dev --inspect app/index",
    "verbose": "cross-env DEBUG=* ",
    "license-report": "license-report"
  },
  "dependencies": {
    "abc4": "^0.10.0",
    "ace-builds": "^1.4.3",
    "arssh2-client": "^1.6.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.3",
    "chai-json-schema": "^1.5.0",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "jquery": "^3.2.1",
    "jquery-contextmenu": "^2.6.2",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.1.4",
    "jstree": "^3.3.4",
    "klaw": "^2.1.1",
    "log4js": "^3.0.6",
    "minimatch": "^3.0.4",
    "nodegit": "^0.23.0",
    "npm": "^6.4.1",
    "nunjucks": "^3.1.4",
    "opener": "^1.4.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path-is-inside": "^1.0.2",
    "promise-retry": "^1.1.1",
    "simple-batch-system": "^1.5.4",
    "socket.io": "^2.1.1",
    "socketio-file-upload": "^0.6.0",
    "split.js": "^1.3.5",
    "ssh2": "^0.5.5",
    "stat-mode": "^0.2.2",
    "svg.draggable.js": "^2.2.1",
    "svgjs": "^2.6.2",
    "uuid": "^3.1.0",
    "viewerjs": "^1.3.2",
    "vue": "^2.6.8",
    "vuetify": "^1.5.5",
    "wait-on": "^2.1.2"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "chai-iterator": "^3.0.0",
    "chai-webdriverio": "^0.4.3",
    "chokidar-cli": "^1.2.2",
    "chromedriver": "^2.40.0",
    "cross-env": "^5.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.4.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "jsdoc-to-markdown": "^4.0.1",
    "json-loader": "^0.5.7",
    "license-report": "^2.1.3",
    "markdown-pdf": "^9.0.0",
    "marked": "^0.5.2",
    "material-design-icons-iconfont": "^4.0.5",
    "mocha": "^5.0.0",
    "nlf": "^2.0.0",
    "node-dev": "^3.1.3",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.3.0",
    "rewire": "^3.0.2",
    "selenium-server": "^3.13.0",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.6.2",
    "wdio-dot-reporter": "0.0.10",
    "wdio-json-reporter": "^0.3.1",
    "wdio-junit-reporter": "^0.4.4",
    "wdio-mocha-framework": "^0.6.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.0",
    "webpack": "=3.12.0"
  }
}
