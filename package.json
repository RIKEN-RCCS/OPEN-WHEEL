{
  "name": "WHEEL",
  "version": "1.2.3",
  "description": "Scientific Workflow system",
  "main": "app/index.js",
  "bin": {
    "wheel": "app/cmd.js"
  },
  "author": {
    "name": "Kenji Ono"
  },
  "contributors": [
    "Tomohiro Kawanabe",
