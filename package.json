    "node": ">=8"
  },
  "scripts": {
    "start": "node app/index",
    "memoryInspection": "run-s memoryMonitor",
    "memoryMonitor": "cross-env NODE_ENV=development MEMORY_MONITOR=TRUE node app/index",
    "profiler": "node --prof app/index",
    "mocha": "mocha",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "prepare": "webpack",
    "dev": "run-p node-dev watch",
    "dev-inspect": "run-p node-dev-inspect watch",
    "node-dev": "node-dev app/index",
    "node-dev-inspect": "node-dev --inspect app/index",
    "watch": "webpack -w",
    "verbose": "cross-env DEBUG=* node-dev --inspect app/index",
    "license-report": "license-report"
  },
